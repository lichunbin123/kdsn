<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.usping.kdsn.mapper.SourceMapper">
    <resultMap id="BaseResultMap" type="com.usping.kdsn.bean.Source">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="apply_user" jdbcType="INTEGER" property="applyUser"/>
        <result column="site_name" jdbcType="INTEGER" property="siteName"/>
        <result column="apply_status" jdbcType="VARCHAR" property="applyStatus"/>
        <result column="apply_handler" jdbcType="INTEGER" property="apply_handler"/>
    </resultMap>
    <sql id="Base_Column_List">
      id, apply_user, site_name, apply_status, apply_handler
    </sql>


    <insert id="insertSelective" useGeneratedKeys="true" keyColumn="message_id" keyProperty="message.messageId">

        insert into Source(
        <include refid="Base_Column_List"/>
        )
        values (
        #{id},
        #{apply_user},
        #{site_name},
        #{apply_satus},
        #{apply_handler}
        )
    </insert>

    <select id="selectAll">

    </select>

</mapper>